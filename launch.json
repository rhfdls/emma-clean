{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Emma",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Emma.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "AZURE_STORAGE_CONNECTION_STRING": "UseDevelopmentStorage=true",
                "DEBUG": "true"
            }
        }
    ]
}
